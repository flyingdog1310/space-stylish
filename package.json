{
  "name": "space-stylish",
  "version": "1.0.0",
  "description": "e-commerce website with MVC architecture",
  "type": "module",
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/flyingdog1310/space-stylish.git"
  },
  "scripts": {
    "start": "node --env-file=.env src/app.js",
    "dev": "node --env-file=.env src/app.js",
    "dev:legacy": "node --env-file=.env app.js",
    "test": "node tests/run-tests.js",
    "test:api": "node tests/api-test-suite.js",
    "test:swagger": "node tests/swagger-validator.js",
    "test:cache": "node --env-file=.env tests/test-cache-fault-tolerance.js",
    "test:all": "node tests/run-tests.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "db:test": "node --env-file=.env -e \"import('./config/database.js').then(db => db.testDatabaseConnections())\""
  },
  "author": "flyingdog1310",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "argon2": "^0.41.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.1.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "axios": "^1.6.0",
    "eslint": "^8.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
